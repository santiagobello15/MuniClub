{
  "builds": [
    {
      "src": "MuniClub/asgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "MuniClub/asgi.py"
    },
    {
      "src": "/ws/(.*)",
      "dest": "MuniClub/asgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "MuniClub/asgi.py"
    }
  ]
}
